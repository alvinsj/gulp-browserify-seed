{
  "name": "gulp-browserify-seed",
  "version": "0.0.0",
  "description": "seed for gulp, browserify",
  "author": {
    "name": "alvinsj",
    "url": "https://github.com/alvinsj"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvinsj/gulp-browserify-seed.git"
  },
  "license": "MIT",
  "keywords": [
    "gulp",
    "browserify",
    "react",
    "jest",
    "bower",
    "sass"
  ],
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "babelify": "^6.1.2",
    "browserify": "7.0.0",
    "cli-color": "^0.3.2",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-babel": "^5.2.1",
    "gulp-bower": ">0.0.7",
    "gulp-concat": "^2.4.2",
    "gulp-load-plugins": "^0.10.0",
    "gulp-rename": "^1.2.0",
    "gulp-ruby-sass": "1.0.0-alpha",
    "gulp-run": "^1.6.6",
    "gulp-shell": "^0.2.11",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-util": "^3.0.1",
    "jest-cli": "alvinsj/jest#default-module-path",
    "npm": "^2.14.0",
    "opener": "^1.4.0",
    "run-sequence": "^1.0.2",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.2.1",
    "yargs": "^3.21.0"
  },
  "dependencies": {
    "react": "^0.13.3"
  },
  "scripts": {
    "test": "NODE_PATH=src/js:node_modules jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/__tests__"
    ],
    "testPathIgnorePatterns": [
      "pending"
    ],
    "unmockedModulePathPatterns": [
      "node_modules",
      "react",
      "react/addons",
      "utils",
      "__mocks__"
    ],
    "modulePathIgnorePatterns": []
  }
}
